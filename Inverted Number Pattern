public class Inverted_halfpyramid_num {
    public static void main(String[] args) {
        int n = 6;
        for(int line = 1; line <= n; line++){
            for(int number = 1;number<=(n-line+1);number++){
                System.out.print(number);
            }
            System.out.println();
        }
    }
}

--------------------------------------------------------------------
Output
--------------------------------------------------------------------
123456
12345
1234
123
12
1
